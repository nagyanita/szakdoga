{
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "worker": true,
        "es6": true
    },
    "globals": {
        "manager": true,
        "chrome": false,
        "io": false,
        "ss": false,
        "Vue": false,
        "_": false,
        "moment": false,
        "accounting": false,
        "nw": false,
        "async": false,
        "BigNumber": false,
        "validator": false,
        "IDBStore": false,
        "swal": false,
        "Vuex": false,
        "inMemoryCols": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "comma-dangle": [
            1,
            "never"
        ],
        "no-cond-assign": [
            2,
            "always"
        ],
        "no-console": 0,
        "no-debugger": 2,
        "no-constant-condition": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 1,
        "no-func-assign": 2,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 1,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 2,
        "valid-typeof": 2,
        //Best Practices
        "accessor-pairs": 2,
        "array-callback-return": 2,
        "block-scoped-var": 2,
        "consistent-return": 2,
        "curly": 1,
        "default-case": 2,
        "dot-location": [
            2,
            "object"
        ],
        "dot-notation": 2,
        "eqeqeq": 1,
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-case-declarations": 2,
        "no-else-return": 2,
        "no-empty-function": 2,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-label": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 1,
        "no-implicit-coercion": 1,
        "no-implicit-globals": 0,
        "no-implied-eval": 2,
        "no-invalid-this": 0,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 1,
        "no-loop-func": 2,
        "no-magic-numbers": 0,
        "no-multi-spaces": 1,
        "no-multi-str": 0,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 2,
        "no-proto": 2,
        "no-redeclare": [
            2,
            {
                "builtinGlobals": true
            }
        ],
        "no-return-assign": [
            1,
            "always"
        ],
        "no-script-url": 2,
        "no-self-assign": 1,
        "no-self-compare": 1,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unmodified-loop-condition": 2,
        "no-unused-expressions": 2,
        "no-unused-labels": 2,
        "no-useless-call": 0,
        "no-useless-concat": 2,
        "no-void": 2,
        "no-warning-comments": [
            1,
            {
                "terms": [
                    "todo",
                    "fixme"
                ],
                "location": "anywhere"
            }
        ],
        "no-with": 2,
        "radix": [
            2,
            "always"
        ],
        "vars-on-top": 0,
        "wrap-iife": [
            2,
            "outside"
        ],
        "yoda": [
            2,
            "never"
        ],
        //Variables
        "init-declarations": [
            0,
            "always"
        ],
        "no-catch-shadow": 0,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-restricted-globals": 0,
        "no-shadow": [
            2,
            {
                "builtinGlobals": true
            }
        ],
        "no-shadow-restricted-names": 2,
        "no-undef": 1,
        "no-undef-init": 1,
        "no-undefined": 0,
        "no-use-before-define": 0,
        "no-unused-vars": [
            1,
            {
                "vars": "local"
            }
        ],
        //Stylistic Issues
        "array-bracket-spacing": [
            1,
            "never"
        ],
        "block-spacing": 1,
        "brace-style": [
            1,
            "1tbs",
            {
                "allowSingleLine": false
            }
        ],
        "camelcase": [
            1,
            {
                "properties": "always"
            }
        ],
        "comma-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            1,
            "last"
        ],
        "computed-property-spacing": [
            1,
            "never"
        ],
        "consistent-this": [
            1,
            "that"
        ],
        "eol-last": 0,
        "func-names": 1,
        "func-style": 0,
        "id-blacklist": 0,
        "id-length": 0,
        "id-match": 0,
        "indent": [
            1,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "key-spacing": [
            1,
            {
                "singleLine": {
                    "beforeColon": false,
                    "afterColon": true
                },
                "multiLine": {
                    "beforeColon": false,
                    "afterColon": true
                }
            }
        ],
        "keyword-spacing": 1,
        "linebreak-style": [
            1,
            "windows"
        ],
        "lines-around-comment": 0,
        "max-depth": 0,
        "max-len": 0,
        "max-nested-callbacks": [
            1,
            {
                "maximum": 8
            }
        ],
        "max-params": 0,
        "max-statements": 0,
        "new-cap": 1,
        "new-parens": 1,
        "newline-after-var": [
            1,
            "always"
        ],
        "newline-before-return": 0,
        "newline-per-chained-call": 0,
        "no-continue": 0,
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-inline-comments": 0,
        "no-lonely-if": 0,
        "no-mixed-spaces-and-tabs": [
            1,
            "smart-tabs"
        ],
        "no-multiple-empty-lines": [
            1,
            {
                "max": 1,
                "maxEOF": 0,
                "maxBOF": 0
            }
        ],
        "no-negated-condition": 0,
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-plusplus": 0,
        "no-restricted-syntax": [
            2,
            "WithStatement"
        ],
        "no-spaced-func": 0,
        "no-ternary": 2,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 2,
        "no-whitespace-before-property": 1,
        "object-curly-spacing": [
            1,
            "always"
        ],
        "one-var": [
            2,
            "always"
        ],
        "one-var-declaration-per-line": 1,
        "operator-assignment": [
            1,
            "always"
        ],
        "operator-linebreak": [
            1,
            "after"
        ],
        "padded-blocks": [
            1,
            "never"
        ],
        "quote-props": [
            1,
            "as-needed"
        ],
        "quotes": [
            1,
            "single",
            "avoid-escape"
        ],
        "require-jsdoc": 1,
        "semi": [
            1,
            "always"
        ],
        "semi-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "sort-imports": 0,
        "sort-vars": 0,
        "space-before-blocks": [
            1,
            "always"
        ],
        "space-before-function-paren": [
            1,
            "never"
        ],
        "space-in-parens": [
            1,
            "never"
        ],
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "spaced-comment": [
            1,
            "never"
        ],
        "wrap-regex": 1
    }
}